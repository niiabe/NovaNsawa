@code {
    private ContactFormModel contactForm = new();
    private bool isSubmitting = false;
    private string? statusMessage;
    private bool isSuccess = false;

    private async Task HandleContactForm()
    {
        isSubmitting = true;
        statusMessage = null;

        try
        {
            // Simulate network delay
            await Task.Delay(2000);

            // Here you would typically send an email or save the form data to a database.
            // For this example, we'll just display a success message.

            isSuccess = true;
            statusMessage = "Your message has been sent. Thank you!";
            contactForm = new(); // Clear the form
        }
        catch (Exception ex)
        {
            isSuccess = false;
            statusMessage = $"An error occurred: {ex.Message}";
        }
        finally
        {
            isSubmitting = false;
        }
    }
}


@* code *@
@code {
    [Inject] private EmailService EmailService { get; set; } = default!;
    
    private ContactFormModel contactForm = new();
    private bool isSubmitting = false;
    private string statusMessage = "";
    private bool isSuccess = false;

    private async Task HandleContactForm()
    {
        isSubmitting = true;
        statusMessage = "";
        StateHasChanged(); // Update UI to show "Sending..." button
        
        try
        {
            await EmailService.SendContactEmailsAsync(contactForm);
            
            statusMessage = "Thank you! Your message has been sent successfully.";
            isSuccess = true;
            
            // Reset form
            contactForm = new ContactFormModel();
        }
        catch (Exception ex)
        {
            // Log the actual error for debugging (optional)
            Console.WriteLine($"Email error: {ex.Message}");
            
            statusMessage = "Sorry, there was an error sending your message. Please try again.";
            isSuccess = false;
        }
        finally
        {
            isSubmitting = false;
            StateHasChanged(); // Update UI to remove "Sending..." state
        }
    }
}













